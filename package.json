{
  "name": "vue-template",
  "version": "0.0.0",
  "scripts": {
    "prepare": "husky install",
    "dev": "vite",
    "build": "yarn run build:style && vue-tsc --noEmit && vite build",
    "build:style": "rimraf ./theme/lib && gulp -f ./theme/gulpfile.js build",
    "build:vue": "vue-tsc --noEmit && vite build"
  },
  "dependencies": {
    "element-plus": "^1.1.0-beta.20",
    "mitt": "^3.0.0",
    "throttle-debounce": "^3.0.1",
    "vue": "^3.2.20",
    "vue-i18n": "^9.1.7",
    "vue-router": "^4.0.11",
    "vuex": "^4.0.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-conventional": "^13.1.0",
    "@intlify/vite-plugin-vue-i18n": "^2.4.0",
    "@types/jest": "^27.0.2",
    "@types/node": "^16.9.6",
    "@typescript-eslint/eslint-plugin": "^4.31.2",
    "@typescript-eslint/parser": "^4.31.2",
    "@vitejs/plugin-vue": "^1.9.0",
    "@vitejs/plugin-vue-jsx": "^1.1.8",
    "@vue/compiler-sfc": "^3.2.14",
    "@vue/test-utils": "^1.2.2",
    "commitizen": "^4.2.4",
    "cz-conventional-changelog": "^3.3.0",
    "cz-customizable": "^6.3.0",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-jest": "^24.4.2",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^7.18.0",
    "gulp": "^4.0.2",
    "gulp-autoprefixer": "^8.0.0",
    "gulp-cssmin": "^0.2.0",
    "gulp-rename": "^2.0.0",
    "gulp-sass": "^5.0.0",
    "husky": "^7.0.2",
    "jest": "^27.2.1",
    "lint-staged": "^11.1.2",
    "prettier": "^2.4.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.58.0",
    "sass": "^1.43.2",
    "ts-jest": "^27.0.5",
    "typescript": "^4.4.3",
    "vite": "^2.5.10",
    "vite-plugin-imp": "^2.0.9",
    "vue-jest": "^3.0.7",
    "vue-tsc": "^0.3.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-customizable"
    }
  },
  "lint-staged": {
    "*.{vue,ts,tsx,jsx,js}": "eslint --fix"
  }
}
